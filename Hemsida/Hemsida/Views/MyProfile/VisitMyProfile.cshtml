@model Hemsida.Data.DAL.UserInfo
@{
    ViewBag.Title = "VisitMyProfile";
}


@using (Html.BeginForm("GetUsersWithInterests", "MyProfile", FormMethod.Post))
{
    <h1> @Model.userName
    <img src="@Url.Action("UserPhotos", "MyProfile" )" style="width:160px;height:160px; background: #FFFFFF;
    margin: auto;
    -moz-border-radius: 60px;
    border-radius: 100px;
    padding: 6px;
    box-shadow: 0px 0px 20px #888;" />
    </h1>

    <div class="col-md-12">
        <h2>Leta efter gemensamma intressen med lagkamrater!</h2>

        <input class="btn btn-default" type="submit" value="Matcha" />
        <br />

    </div>

}
